{
    "plugins": ["plugins/markdown"],
    "source": {
        "include": [
            "./bin",
            "./lib",
            "./README.md"
        ],
        "includePattern": "\\.js$"
    },
    "opts": {
        "template": "templates/default",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true
    }
}
