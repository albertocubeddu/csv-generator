{
    "name": "csv-generator",
    "version": "2.0.0-dev.3",
    "main": "index.js",
    "repository": "https://github.com/atomicpages/csv-generator.git",
    "author": "Dennis Thompson <djtthompson@gmail.com>",
    "license": "MIT",
    "bin": "./bin/gencsv.js",
    "preferGlobal": true,
    "scripts": {
        "test": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
        "coverage": "nyc mocha --bail",
        "lint": "./node_modules/.bin/eslint --max-warnings 2 **/*.js",
        "docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
        "e2e": ""
    },
    "engines": {
        "node": ">=8.6.0"
    },
    "dependencies": {
        "chalk": "^2.4.1",
        "chance": "^1.0.16",
        "clear": "^0.1.0",
        "cli-table": "^0.3.1",
        "csv-string": "^3.1.5",
        "faker": "^4.1.0",
        "figlet": "^1.2.0",
        "i18n": "^0.8.3",
        "inquirer": "^6.2.0",
        "is-plain-object": "^2.0.4",
        "lodash": "^4.17.10",
        "moment": "^2.22.2",
        "progress": "^2.0.0",
        "uuid": "^3.3.2",
        "yargs": "^12.0.2"
    },
    "devDependencies": {
        "chai": "^4.1.2",
        "coveralls": "^3.0.2",
        "eslint": "^5.5.0",
        "eslint-plugin-chai-expect": "^1.1.1",
        "eslint-plugin-mocha": "^5.2.0",
        "eslint-plugin-promise": "^4.0.1",
        "istanbul": "^0.4.5",
        "jsdoc": "^3.5.5",
        "mocha": "^5.2.0",
        "mocha-lcov-reporter": "^1.3.0",
        "mocha-sinon": "^2.0.0",
        "mock-fs": "^4.4.1",
        "mock-stdin": "^0.3.1",
        "nyc": "^13.0.1",
        "sinon": "^6.2.0",
        "strip-ansi": "^4.0.0"
    },
    "nyc": {
        "exclude": [
            "**/vendor/**/*.js",
            "test",
            "test{,-*}.js",
            "**/*.test.js",
            "**/__tests__/**",
            "**/node_modules/**"
        ],
        "reporter": [
            "text",
            "html"
        ],
        "check-coverage": true,
        "per-file": true,
        "lines": 90,
        "statements": 90,
        "branches": 90,
        "functions": 80
    }
}
